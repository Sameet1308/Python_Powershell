WITH months AS (
    SELECT 
        ROW_NUMBER() OVER (ORDER BY SEQ4()) AS month_num
    FROM 
        TABLE(GENERATOR(ROWCOUNT => 12))
)
SELECT 
    LPAD(month_num::STRING, 2, '0') AS MONTH_PADDED,
    month_num AS MONTH_NUMBER,
    TO_CHAR(DATE_TRUNC('MONTH', DATEADD('MONTH', month_num - 1, '2022-01-01')), 'Month') AS MONTH_NAME
FROM 
    months
ORDER BY 
    MONTH_NUMBER DESC;
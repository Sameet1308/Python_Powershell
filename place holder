# Define the path to the log file
log_file_path = '/path/to/your/log_file.log'  # Replace with the actual path to your log file

# Function to read and print the contents of the log file
def read_log_file(file_path):
    try:
        with open(file_path, 'r') as file:
            # Read the file contents
            log_contents = file.read()
            # Print the contents
            print(log_contents)
    except Exception as e:
        print(f"An error occurred: {e}")

# Call the function to read and print the log file
read_log_file(log_file_path)